# Convenience
alias py='python3'
alias tree='tree -I env'

# Directory shortcuts 
alias screeps="cd ~/.config/Screeps/scripts/screeps_newbieland_net___21025/default/"

# Vim to Nvim
alias vim='nvim'

# Creates parent directories if they don't exist by default with mkdir
alias mkdir='mkdir -p'

# Ask if you want to overrwrite when moving a file if it will
alias mv='mv -i'

# Protects against typos
alias sl='ls'
alias dc='cd'

# git shortcuts
alias gs='git status'
